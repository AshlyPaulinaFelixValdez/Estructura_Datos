// recorrido de arreglo
using system;
class program
{
    static void main()
    {
        int[] numeros = { 10, 20, 30, 40 };

        // for normal
        for (int i = 0; i < numeros.length; i++)
        {
            console.writeline(numeros[i]);
        }

        // foreach
        foreach (int n in numeros)
        {
            console.writeline(n);
        }
    }
}